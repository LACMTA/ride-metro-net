---
import Layout from '../layouts/Layout.astro';
import { openDb } from 'gtfs';
import { GTFSconfig } from '../integrations/import-gtfs';
export const prerender = true;

const db = openDb(GTFSconfig);

const results = await db.prepare('SELECT * from calendar_dates LIMIT 10;').all();

const tables = await db.prepare("SELECT name FROM sqlite_master WHERE type='table' AND name NOT LIKE 'sqlite_%'").all();


---

<Layout>
	<code>
		{JSON.stringify(tables)}
		{JSON.stringify(results)}
	</code>
</Layout>
